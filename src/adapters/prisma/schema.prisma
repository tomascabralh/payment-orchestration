generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PaymentOrder {
  uuid           String   @id @default(uuid())
  amount         Int
  description    String
  countryIsoCode String
  createdAt      DateTime @default(now())
  paymentUrl     String
}
